{
  "name": "app-demo-fancy",
  "version": "1.0.0",
  "description": "Run",
  "main": "Main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "elm-live -d public/ src/Main.elm -- --output public/Main.js",
    "debug": "elm make --debug src/Main.elm --output public/Main.js",
    "compile": "elm make --optimize src/Main.elm --output public/Main.js",
    "ff": "open -a /Applications/Firefox.app/ public/index.html",
    "chrome": "open -a /Applications/GoogleChrome.app/ public/index.html",
    "set-dev": "cp elm-dev.json elm.json",
    "set-pub": "cp elm-pub.json elm.json",
    "update-pub": "cp elm.json elm-pub.json",
    "publish": "sh scripts/publish.sh",
    "go": "open -a /Applications/Firefox.app/ https://markdown.minilatex.app/"
  },
  "keywords": [],
  "author": "",
  "license": "BSD-3",
  "devDependencies": {
    "elm": "^0.19.1-3",
    "elm-live": "^4.0.1"
  }
}
